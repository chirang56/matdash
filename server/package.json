{
  "name": "server",
  "version": "1.0.0",
  "description": "Backend server with Prisma ORM, Express, and MySQL integration",
  "main": "server.js",
  "scripts":{ 
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prisma:generate": "prisma generate",
    "prisma:init": "prisma init", 
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:reset": "prisma migrate reset --force",
    "prisma:db-pull": "prisma db pull", 
    "prisma:studio": "prisma studio",
    "test": "echo \"Error: no test specified\" && exit 1"
  },

  "keywords": ["Prisma", "Express", "MySQL", "ORM", "Backend"],
  "author": "CheeRunGB",
  "license": "ISC",
  "dependencies":{
    "@prisma/client": "^5.22.0",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "mysql2": "^3.11.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.7",
    "prisma": "^5.22.0"
  }
}
